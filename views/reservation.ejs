<body>


    <section class="reservation reservation-bg">
        <div class="container">
            <div class="row">
                <h1 class="text-center text-dark">RESERVATIONS</h1>
                <h3 class="text-center">Book a table online. Leads will reach in your email.</h3>
            </div>
            <div class="reservation-form">
                <div class="row">
                    <div class="container">
                        <div class="reservation-form-inner">
                            <form action="" method="">
                                <div class="col-md-4 col-sm-6">

                                    <!-- <div class="form-group">
                                                        <label for="">enter date</label>
                                                            
                                                            <input type="text"style="color:black " class="form-control" name="date"
                                                                placeholder="" required>
                                                        </div> -->


                                    <div class="form-group">
                                        <label for="date">Date</label>
                                        <input type="date" style="color:black " class="form-control" name="date"
                                            placeholder="" required>
                                    </div>

                                </div>
                                <div class="col-md-4 col-sm-6">
                                    <div class="form-group">
                                        <label for="">Your Name</label>
                                        <input type="text" style="color:black " class="form-control" name="u_name"
                                            placeholder="Enter your name" required>
                                    </div>

                                </div>


                                <!-- <div class="form-group">
                                                        <label for="">enter time</label>
                                                            
                                                            <input type="text" style="color:black "class="form-control" name="time"
                                                                placeholder="" required>
                                                        </div>
 -->

                                <div class="col-md-4 col-sm-6">
                                    <div class="form-group">
                                        <label for="">Time</label>
                                        <input type="text" style="color:black " class="form-control" name="time"
                                            placeholder="Pick a time" required>
                                    </div>

                                </div>
                                <div class="col-md-4 col-sm-6">
                                    <div class="form-group">
                                        <label for="">Email Address</label>
                                        <input type="text" style="color:black " class="form-control" name="u_email"
                                            placeholder="Enter email address" required>
                                    </div>

                                </div>

                                <div class="col-md-4 col-sm-6">
                                    <div class="form-group">
                                        <label for="">Guests</label>
                                        <input type="text" style="color:black " class="form-control" name="total_guest"
                                            placeholder="How many of you?">
                                    </div>

                                </div>
                                <div class="col-md-4 col-sm-6">
                                    <div class="form-group">
                                        <label for="">Phone Number</label>
                                        <input type="text" style="color:black " class="form-control" name="phone_no"
                                            placeholder="Enter your phone number">
                                    </div>



                                </div>
                                <button type="submit" class="btn btn-primary">Make reservation</button>
                            </form>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </section>



